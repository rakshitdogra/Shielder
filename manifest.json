{
    "name": "SHIELDER", 
    "description": "Adblocker",
    "manifest_version": 2,
    "version": "1.0",
    "background": {
        "scripts": ["site.js", "Block.js"]
    },

    "browser_action": {
        "default_popup": "pop.html"

    },  
    "permissions": ["webRequest", "webRequestBlocking", "http://*/", "https://*/"]
}
